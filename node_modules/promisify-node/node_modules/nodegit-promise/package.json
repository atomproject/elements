  "scripts": {
    "prepublish": "node build",
    "pretest": "node build",
    "pretest-resolve": "node build",
    "pretest-extensions": "node build",
    "pretest-memory-leak": "node build",
    "test": "mocha --bail --timeout 200 --slow 99999 -R dot && npm run test-memory-leak",
